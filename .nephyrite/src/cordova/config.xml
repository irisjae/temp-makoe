<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.404" android-versionCode="0000" version="0.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>name here</name>
    <description>description here</description>
    <author email="example@example.com" href="http://www.example.com">author here</author>
    <content src="index.html" />
    <preference name="target-device" value="handset" />
    <preference name="Orientation" value="portrait" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="Fullscreen" value="true" />
    <gap:config-file overwrite="true" parent="UIViewControllerBasedStatusBarAppearance" platform="ios">
        <false />
    </gap:config-file>
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="instagram://*" />
    <allow-intent href="fb://*" />
    <allow-intent href="mailto:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-hidden-statusbar-overlay" spec="git+https://github.com/katzer/cordova-plugin-hidden-statusbar-overlay.git" />
    <engine name="browser" spec="^5.0.1" />
    <engine name="android" spec="~7.0.0" />
    <engine name="ios" spec="~4.5.4" />
</widget>
